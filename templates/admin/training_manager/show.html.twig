{% extends './admin/base.html.twig' %}

{% block title %}TrainingManager{% endblock %}

{% block body %}
    <h1>TrainingManager</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ training_manager.id }}</td>
            </tr>
            <tr>
                <th>First_name</th>
                <td>{{ training_manager.firstName }}</td>
            </tr>
            <tr>
                <th>Last_name</th>
                <td>{{ training_manager.lastName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ training_manager.email }}</td>
            </tr>
            <tr>
                <th>Is_active</th>
                <td>{{ training_manager.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ training_manager.createdAt ? training_manager.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ training_manager.updatedAt ? training_manager.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ training_manager.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ training_manager.updatedBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_training_manager_index') }}">back to list</a>

    <a href="{{ path('admin_training_manager_edit', {'id': training_manager.id}) }}">edit</a>

    {{ include('admin/training_manager/_delete_form.html.twig') }}
{% endblock %}
