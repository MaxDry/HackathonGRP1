            <tr>
                <th>CreatedBy</th>
                <td>{{ training_measure.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ training_measure.updatedBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_training_measure_index') }}">back to list</a>

